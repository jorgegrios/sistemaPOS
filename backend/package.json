{
  "name": "sistema-pos-backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Add tests\" && exit 0",
    "test:e2e": "playwright test",
    "test:load": "k6 run k6-load-test.js",
    "migrate": "node-pg-migrate",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:reset": "node-pg-migrate down --count 999 && node-pg-migrate up",
    "seed": "ts-node scripts/seed.ts",
    "seed:complete": "ts-node scripts/seed-complete.ts"
  },
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.0",
