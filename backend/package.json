    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "bcrypt": "^6.0.0",
    "bonjour-service": "^1.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.3",
    "mdns": "^2.7.2",
    "mercadopago": "^2.0.0",
    "node-thermal-printer": "^4.5.0",
    "openai": "^4.104.0",
    "pg": "^8.11.0",
    "socket.io": "^4.8.1",
    "square": "^35.0.0",
    "stripe": "^12.14.0",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@sentry/node": "^10.32.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^20.5.6",
    "@types/pg": "^8.15.6",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "dd-trace": "^5.81.0",
    "eslint": "^8.50.0",
    "node-pg-migrate": "^8.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.4"
  }
